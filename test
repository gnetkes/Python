#3
def area(length,height):
    return length*height

area1 =area(10,5)
area2 = area(20,2)
sum_area = area1 + area2
print("this is the total area dumbass, "+ str(sum_area))

#4
def lucky_number(name):
    number = len(name)*9
    print("hey", name, "your lucky number is",number)

lucky_number('boasbk')
lucky_number('teng')

