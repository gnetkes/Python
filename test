def username_length(username):
    if len(username) > 20:
        print("get ur shit tgt too long")
    elif len(username) <3:
        print("too short broski")
    else: 
        print("goodjob")

username_length('ah8nln;lnjklbbjklbjklbjkllilbkbl')

